@{
}

@model List<Network>

<h1>CityBike Locations in the United States</h1>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Location</th>
        <th>E-Bike Availability</th>
    </tr>

    @foreach (var item in @Model)
    {
        if(item.location.country == "US")
        {
            <tr>
                <td>@item.name</td>
                <td>@item.location.city</td>
                <td>@item.ebikes</td>
            </tr>
        }
       
    }

</table>